class Solution {
    public int fib(int n) {

        int a = 0;
        int b = 1;
        int res = 0;
        if(n == 2){
            return 1;
        }
        if(n==1){
            return 1;
        }
        for(int i=1;i<n;i++){
            res = b+a;
            a = b;
            b = res;


        }
        return res;
    }
}
